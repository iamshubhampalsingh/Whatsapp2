###################################################Testcase 87##################################################################
# Import necessary modules
import uiautomator2 as ui2
import time
import subprocess

# Device serial number of the receiver device
receiver_device_serial = "192.168.2.39:5555"

# Create a Device object for the sender device
sender_device = ui2.Device('cf611ff9')

# Create a Device object for the receiver device
receiver_device = ui2.Device(receiver_device_serial)

# ADB commands to turn on the receiver and sender devices' screens
receiver_turn_on_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '224']
sender_turn_on_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '224']

# ADB commands to turn off the receiver and sender devices' screens
receiver_turn_off_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '26']
sender_turn_off_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '26']

# Package name and activity name of WhatsApp
whatsapp_package_name = "com.whatsapp"
whatsapp_activity_name = "com.whatsapp.Main"

# Start WhatsApp using the package name and activity name
sender_device.app_start(whatsapp_package_name, whatsapp_activity_name)

receiver_device.press.home()  # Press the home button
receiver_device(scrollable=True).scroll.to(text="Clock").click()  # Open the Clock app
receiver_device(resourceId="com.google.android.deskclock:id/tab_menu_timer").click()
time.sleep(2)
receiver_device(resourceId="com.google.android.deskclock:id/fab").click()
time.sleep(2)
receiver_device(resourceId="com.android.systemui:id/center_group").click()
time.sleep(2)
receiver_device(resourceId="android:id/action0", text="STOP").click()
print("The timer menu is stopped")

############################################Testcase 90######################################################################
# Import necessary modules
import uiautomator2 as ui2
import time
import subprocess

# Device serial number of the receiver device
receiver_device_serial = "192.168.2.39:5555"

# Create a Device object for the sender device
sender_device = ui2.Device('cf611ff9')

# Create a Device object for the receiver device
receiver_device = ui2.Device(receiver_device_serial)

# ADB commands to turn on the receiver and sender devices' screens
receiver_turn_on_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '224']
sender_turn_on_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '224']

# ADB commands to turn off the receiver and sender devices' screens
receiver_turn_off_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '26']
sender_turn_off_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '26']

# Package name and activity name of WhatsApp
whatsapp_package_name = "com.whatsapp"
whatsapp_activity_name = "com.whatsapp.Main"

# Start WhatsApp using the package name and activity name
sender_device.app_start(whatsapp_package_name, whatsapp_activity_name)

receiver_device.press.home()  # Press the home button
receiver_device(scrollable=True).scroll.to(text="Calendar")
time.sleep(2)
receiver_device(resourceId="com.android.launcher3:id/icon", text="Calendar").click()
time.sleep(2)
receiver_device(resourceId="com.google.android.calendar:id/floating_action_button").click()
time.sleep(2)
receiver_device(resourceId="com.google.android.calendar:id/floating_action_button").click()
time.sleep(2)
receiver_device(resourceId="com.google.android.calendar:id/title").set_text("Event for mobile")
time.sleep(2)
receiver_device(resourceId="com.google.android.calendar:id/og_apd_internal_image_view").click()
time.sleep(2)
receiver_device(resourceId="com.android.systemui:id/center_group").click()
time.sleep(2)
print("The calendar event added with whatsapp")

#####################################################Whatsapp 91##############################################################
# Import necessary modules
import uiautomator2 as ui2
import time
import subprocess

# Device serial number of the receiver device
receiver_device_serial = "192.168.2.39:5555"

# Create a Device object for the sender device
sender_device = ui2.Device('cf611ff9')

# Create a Device object for the receiver device
receiver_device = ui2.Device(receiver_device_serial)

# ADB commands to turn on the receiver and sender devices' screens
receiver_turn_on_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '224']
sender_turn_on_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '224']

# ADB commands to turn off the receiver and sender devices' screens
receiver_turn_off_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '26']
sender_turn_off_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '26']

# Package name and activity name of WhatsApp
whatsapp_package_name = "com.whatsapp"
whatsapp_activity_name = "com.whatsapp.Main"

# Start WhatsApp using the package name and activity name
sender_device.app_start(whatsapp_package_name, whatsapp_activity_name)

receiver_device.press.home()  # Press the home button
receiver_device(scrollable=True).scroll.to(text="Wynk Music")
time.sleep(2)
receiver_device(resourceId="com.bsbportal.music:id/bgImage").click()
time.sleep(2)
receiver_device(resourceId="com.android.systemui:id/center_group").click()

# Package name and activity name of WhatsApp
whatsapp_package_name = "com.whatsapp"
whatsapp_activity_name = "com.whatsapp.Main"

# Start WhatsApp using the package name and activity name
sender_device.app_start(whatsapp_package_name, whatsapp_activity_name)
print("whatsApp is working fine")

#############################################Whatsapp 92 ###################################################################

# Import necessary modules
import uiautomator2 as ui2
import time
import subprocess

# Device serial number of the receiver device
receiver_device_serial = "192.168.2.39:5555"

# Create a Device object for the sender device
sender_device = ui2.Device('cf611ff9')

# Create a Device object for the receiver device
receiver_device = ui2.Device(receiver_device_serial)

# ADB commands to turn on the receiver and sender devices' screens
receiver_turn_on_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '224']
sender_turn_on_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '224']

# ADB commands to turn off the receiver and sender devices' screens
receiver_turn_off_command = ['adb', '-s', receiver_device_serial, 'shell', 'input', 'keyevent', '26']
sender_turn_off_command = ['adb', '-s', 'cf611ff9', 'shell', 'input', 'keyevent', '26']

# Package name and activity name of WhatsApp
whatsapp_package_name = "com.whatsapp"
whatsapp_activity_name = "com.whatsapp.Main"

# Start WhatsApp using the package name and activity name
sender_device.app_start(whatsapp_package_name, whatsapp_activity_name)

receiver_device.press.home()  # Press the home button
# Define the initial swipe coordinates
start_x = 500
start_y = 500
end_y = 1500
swipe_duration = 0.5

# Perform multiple swipes to scroll the screen downwards
num_swipes = 5  # Adjust the number of swipes as needed
for _ in range(num_swipes):
    receiver_device.swipe(start_x, start_y, start_x, end_y, duration=swipe_duration)
    time.sleep(1)  # Adjust the delay between swipes if necessary
receiver_device(text="Off", description="Bluetooth").click()
time.sleep(2)    
receiver_device(text="Off", description="Bluetooth").long_click()
time.sleep(2)
receiver_device.press.home()  # Press the home button
receiver_device(scrollable=True).scroll.to(text="Files")
time.sleep(2)
receiver_device(text="Files").click()
time.sleep(2)
receiver_device(text="Audio").click()
time.sleep(2)
receiver_device(resourceId="com.google.android.apps.nbu.files:id/thumbnail").long_click()
time.sleep(2)
receiver_device(resourceId="com.google.android.apps.nbu.files:id/share_action").click()
time.sleep(2)
receiver_device.xpath('//*[@resource-id="android:id/resolver_list"]/android.widget.LinearLayout[3]/android.widget.LinearLayout[1]/android.widget.ImageView[1]').click()
time.sleep(2)
receiver_device(resourceId="android:id/title", text="OnePlus 9 Pro 5G").click()
time.sleep(2)
sender_device(text="ACCEPT").click()
time.sleep(10)
print("The audio file is shared using bluetooth")

