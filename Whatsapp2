        time.sleep(1)
        receiver_device(text="Pair").click()
        print("Bluetooth connected")
        break
    else:
        print("Bluetooth not connected")
    time.sleep(1)

# Increase the volume
increase_command = "adb shell input keyevent 24"
subprocess.run(increase_command, shell=True)


# Decrease the volume
decrease_command = "adb shell input keyevent 25"
subprocess.run(decrease_command, shell=True)
                
